#include<bits/stdc++.h>
using namespace std;
 
 
void  Swap(int A[],int n)
{
    int temp,i=n;
    temp=A[n];
    while(i>1&&temp>A[i/2])
    {
       A[i]=A[i/2];
       i=i/2;
    }
    A[i]=temp;
}

void createHeap()
{
   cout<<"Enter the number elements";
   int n;
   cin>>n;
   int A[n+1];
   cout<<"Enter the elements of the Array";
   for(int i=1;i<=n;i++)
   {
      cin>>A[i];
   }
    
    for(int i=2;i<=n;i++)
    {
    
    Swap(A,n);
    }
    
}    
